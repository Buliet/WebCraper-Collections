{
    "_id":"youtube_playlists",
    "startUrl":[
        "https://www.youtube.com/c/SteveTVShow/playlists"
    ],
    "selectors":[
        {
            "id":"ele-playlists",
            "parentSelectors":[
                "_root"
            ],
            "type":"SelectorElementScroll",
            "selector":"ytd-grid-playlist-renderer",
            "multiple":true,
            "delay":2000
        },
        {
            "id":"title-playlist",
            "parentSelectors":[
                "ele-playlists"
            ],
            "type":"SelectorText",
            "selector":"a.ytd-grid-playlist-renderer",
            "multiple":false,
            "delay":0,
            "regex":""
        },
        {
            "id":"link-playlist",
            "parentSelectors":[
                "ele-playlists"
            ],
            "type":"SelectorLink",
            "selector":"a.yt-formatted-string",
            "multiple":false,
            "delay":0
        }
    ]
}
